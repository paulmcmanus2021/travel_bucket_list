<h1>Cities Added</h1>


<% for city in @cities %>
  <p><%= city.name() %></p>
  <button onclick="window.location.href = '/cities/<%= city.id() %>/edit';">Edit City</button>
  <form action="/cities/<%= city.id %>/delete" method="POST">
    <input type="submit" value="Delete City">
  </form>


<% end %>
